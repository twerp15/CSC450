#include <iostream>
#include <string>

int main() {
    std::string firstName = "Jeff";
    std::string lastName = "Dom";
    std::string streetAddress = "5180 Blackmore Rd";
    std::string city = "Evansville";
    std::string zipCode = "806209";

    std::cout << "First Name: " << firstName << std::endl;
    std::cout << "Last Name: " << lastName << std::endl;
    std::cout << "Street Address: " << streetAddress << std::endl;
    std::cout << "City: " << city << std::endl;
    std::cout << "Zip Code: " << zipCode << std::endl;

    return 0;
}
